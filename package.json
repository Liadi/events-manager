{
  "name": "events-manager",
  "version": "1.0.0",
  "description": "Event managing application",
  "main": "index.js",
  "scripts": {
    "test": "mocha bin/test",
    "start": "npm run production",
    "start:dev": "nodemon --exec babel-node bin/dev",
    "clean": "rimraf ./dist",
    "build": "npm run clean mkdir dist && babel server -s -d dist",
    "production": "npm run build && node bin/production",
    "db:init": "sequelize init",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "coverage": "node istanbul cover _mocha -- -R spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Liadi/events-manager.git"
  },
  "author": "Omotola Liadi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Liadi/events-manager/issues"
  },
  "homepage": "https://github.com/Liadi/events-manager#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "mocha": "^4.0.1",
    "rimraf": "^2.6.2",
    "sequelize-cli": "^3.1.0"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.22.9"
  }
}
