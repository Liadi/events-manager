{
  "name": "events-manager",
  "version": "1.0.0",
  "description": "Event managing application",
  "main": "index.js",
  "scripts": {
    "heroku-postbuild": "sequelize db:migrate && rimraf ./dist/server/seeders",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:undo:all && sequelize db:seed:all",
    "seed:testdb": "cross-env NODE_ENV=test sequelize db:seed:undo:all &&cross-env NODE_ENV=test sequelize db:seed:all",
    "test:db": "cross-env NODE_ENV=test sequelize db:migrate:undo:all &&cross-env NODE_ENV=test sequelize db:migrate && npm run seed:testdb",
    "test": "npm run test:db && cross-env NODE_ENV=test mocha  bin/test --exit",
    "start": "npm run production",
    "dev": "cross-env NODE_ENV=development babel-node bin/dev",
    "clean": "rimraf ./dist",
    "build": "npm run clean mkdir -p /dist/server && npm run copy:config && babel server -s -d dist/server",
    "build:bundle": "webpack --config webpack.prod.config.js",
    "copy:config": "cpx \"server/config/config.json\" dist/server/config && cpx \"client/index.html\" dist/client ",
    "production": "npm run build && npm run build:bundle && cross-env NODE_ENV=production babel-node bin/production",
    "db:init": "sequelize init",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "coverage": "node istanbul cover _mocha -- -R spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Liadi/events-manager.git"
  },
  "author": "Omotola Liadi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Liadi/events-manager/issues"
  },
  "homepage": "https://github.com/Liadi/events-manager#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^1.1.6",
    "font-awesome-webpack": "0.0.5-beta.2",
    "html-webpack-plugin": "^2.30.1",
    "mocha": "^4.0.1",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.3",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "sequelize-cli": "^3.1.0",
    "style-loader": "^0.19.1",
    "supertest": "^3.0.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.3",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-plugin-transform-react-jsx-img-import": "^0.1.4",
    "bcrypt": "^1.0.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cpx": "^1.5.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.22.9",
    "superagent": "^3.8.2"
  }
}
